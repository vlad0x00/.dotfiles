Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

URxvt.font: xft:Ubuntu Mono:size=10
URxvt.scrollBar: false
URxvt.termName: xterm-256color

! special
*.foreground:   #c0c5ce
*.background:   #2b303b
